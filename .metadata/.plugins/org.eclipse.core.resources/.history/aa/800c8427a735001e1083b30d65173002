package com.rays.inheritance;

public class Cheque extends Payment{
	
	private String chqNumber;
	private String banckName;
	public String getChqNumber() {
		return chqNumber;
	}
	public void setChqNumber(String chqNumber) {
		this.chqNumber = chqNumber;
	}
	public String getBankName() {
		return banckName;
	}
	public void setBankName(String banckName) {
		this.banckName = banckName;
	}

}
